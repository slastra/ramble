# Password for nuxt-auth-utils (minimum 32 characters)
NUXT_SESSION_PASSWORD=
# GitHub OAuth client ID
NUXT_OAUTH_GITHUB_CLIENT_ID=
# GitHub OAuth client secret
NUXT_OAUTH_GITHUB_CLIENT_SECRET=
# Vercel AI gateway key
AI_GATEWAY_API_KEY=
# Postgres database url
DATABASE_URL=
# Site URL for production
NUXT_PUBLIC_SITE_URL=https://lastra.us
# Ntfy server URL (default: https://ntfy.sh)
NTFY_URL=
# Ntfy topic name
NTFY_TOPIC=
# Your username (notifications won't be sent for messages from this user)
MY_USERNAME=
# Gemini API Configuration
# Get your API key from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash-lite

# WebRTC STUN/TURN Configuration
# STUN server for NAT traversal
NUXT_PUBLIC_STUN_URL=stun:stun.l.google.com:19302

# TURN server for relay (optional but recommended for better connectivity)
# Replace with your own TURN server for production use
NUXT_PUBLIC_TURN_URL=turn:your-turn-server.com:3478
NUXT_PUBLIC_TURN_USERNAME=your-turn-username
NUXT_PUBLIC_TURN_CREDENTIAL=your-turn-password
